{
  "kind": "singleType",
  "collectionName": "projects_pages",
  "info": {
    "singularName": "projects-page",
    "pluralName": "projects-pages",
    "displayName": "ProjectsPage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "unique": false,
      "required": true,
      "default": "My Projects"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "pageInfo": {
      "type": "component",
      "repeatable": false,
      "component": "layout.page-info"
    },
    "excludedProjects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project"
    }
  }
}
